<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Is It ... Yet ?">
    <title>{% block title %}IsItYet{% endblock %}</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    {% block meta %}{% endblock %}

    {% block stylesheets %}{% endblock %}

    <script type="text/javascript">
        var base_url = "{{ app.request.schemeAndHttpHost }}{{ app.environment == 'prod' ? '/' : '/app_dev.php/' }}";
        {% block javascript_head %}
        {% endblock %}
    </script>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>

{% block body %}{% endblock %}

<footer class="footer">
    <div class="container">
        <div class="row">
            {% block footer %}
                <div class="col-md-12 col-xs-12"><p class="text-center">Â© 2015 <a href="http://www.funkybits.fr">FunkyBits</a>&nbsp;&nbsp;<small>(<a href="https://github.com/conradfr/IsItYet">sources</a>)</small></p></div>
            {% endblock %}
        </div>
{#dsfd#}
    </div>
</footer>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
{% block javascripts %}
{% if app.environment == 'prod' %}
<!-- prod:vendors.js -->
<script src="/compiled/js/vendors.js/vendors.js"></script>
<!-- endbuild -->
{% else %}
<!-- dev:vendors.js -->
<script src="/compiled_dev/js/vendors.js/autobahn.min_0.js"></script>
<script src="/compiled_dev/js/vendors.js/spin.min_1.js"></script>
<!-- endbuild -->
{% endif %}
{% endblock %}

{% if app.environment == 'prod' %}
    <!-- prod:bootstrap.min.js -->
<script src="/compiled/js/bootstrap.min.js/bootstrap.min.js"></script>
<!-- endbuild -->
{% else %}
    <!-- dev:bootstrap.min.js -->
<script src="/compiled_dev/js/bootstrap.min.js/bootstrap.min_0.js"></script>
<!-- endbuild -->
{% endif %}

{% if app.environment == 'prod' %}
<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', '{{ analytics }}', 'auto');
    ga('send', 'pageview');
</script>
{% endif %}

{% block react %}{% endblock %}
</body>
</html>